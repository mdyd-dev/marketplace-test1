---
slug: products
---
{% graphql items = 'products' | dig: 'models' %}
<table>
  {% for item in items.results %}
    <tr>
      <td>
        {{ item.id  }}.
      </td>
      <td>
        {{ item.title  }}
      </td>
      <td>
        {{ item.price }}
      </td>
      <td>
        <a href="/products/show/{{item.id}}">
          show
        </a>
      </td>
      <td>
        <a href="/products/edit/{{item.id}}">
          edit
        </a>
      </td>
      <td>
        <a href="/products/delete/{{item.id}}">
          delete
        </a>
      </td>
    </tr>
  {% endfor %}
</table>
